{
  "compilerOptions": {
    "ignoreDeprecations": "6.0",
    "baseUrl": ".",
    "paths": {
      "suada-components/components": ["node_modules/suada-components/dist/components"],
      "suada-components/icons": ["node_modules/suada-components/dist/icons"]
    },
    "target": "es2015",
    "lib": ["dom", "es2015", "es2017"],
    "module": "esnext",
    "jsx": "react-jsx",
    "declaration": true,
    "declarationDir": "dist",
    "rootDir": "src",
    "sourceMap": true,
    "sourceRoot": "",
    "outDir": "dist",
    "strict": false,
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["vite/client", "@storybook/react"]
  },
  "include": [
    "src",
    "src/theme/interfaces.ts",
    "src/theme/Theme.ts",
    "src/theme/ThemeProvider.tsx"
  ],
  "exclude": ["node_modules", "dist", "docs"]
}
