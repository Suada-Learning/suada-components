{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "suada-components/components": ["node_modules/suada-components/dist/components"],
      "suada-components/icons": ["node_modules/suada-components/dist/icons"]
    },
    "target": "ES2018",
    "lib": ["ES2018", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "jsx": "react-jsx",
    "declaration": true,
    "declarationDir": "dist",
    "rootDir": "src",
    "sourceMap": true,
    "sourceRoot": "",
    "outDir": "dist",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["vite/client", "@storybook/react"],
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false
  },
  "include": [
    "src",
    "src/theme/interfaces.ts",
    "src/theme/Theme.ts",
    "src/theme/ThemeProvider.tsx"
  ],
  "exclude": ["node_modules", "dist", "docs"]
}
